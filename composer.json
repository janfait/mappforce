{
    "name": "Mapp Integrator",
    "description": "Mapp Integrator is an environement to set up custom events between Mapp CEP and other platforms, builds heavily on napolux/helloslim3",
    "keywords": ["slimframework", "sfdc", "twig","eloquent", "mapp"],
    "homepage": "https://github.com/janfait",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Fait",
            "email": "jan.fait@mapp.com",
            "homepage": "http://github.com/janfait"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "slim/slim": "^3.5",
        "monolog/monolog": "^1.17",
        "slim/twig-view": "^2.1",
		"slim/csrf":"*",
		"vlucas/phpdotenv":"*",
		"illuminate/database": "*",
		"league/oauth2-client":"*",
		"stevenmaguire/oauth2-salesforce":"*",
        "slim/http-cache": "^0.3.0",
		"developerforce/force.com-toolkit-for-php":"^27.0.0"
    },
    "autoload": {
        "psr-4": {
            "MappIntegrator\\": "src"
        }
    }
}
